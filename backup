                        for(i=1;i<total_images+1;i=i+6){
                            // console.log("yes");
                            img_data+="<div class='col-md-2 col-12'>";
                            for(k=0;k<6 && j<total_images;k++){
                                img_data+="<img src='"+data[3]+"/"+myArr[j]+"' alt=''>";
