                }).ajaxStop(function() {
                    $("#ajax_load").css("z-index","-5");
                });
                $.ajax({
                    url: "https://localhost/python/pdf/extract_image.php",
                    type: 'POST',
                    data: formData,
                    success: function (data) {
                        // alert(data);
                        $('.galary').css("display","block");
                        $('.download_image').css("display","flex");
                        var img_data="";
                        let str = data[1];
                        var myArr = str.split("`");
                        var j=0;
