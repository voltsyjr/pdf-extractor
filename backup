                        $('.galary').html(gallary);
                        var just_download="<a href='"+data[3]+"/"+data[2]+"' target='_blank' >Download Your Images</a>"
                        $('#just_download').html(just_download);
                        start.val("");
                        end.val("");
                        // console.log(data);
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });



            
